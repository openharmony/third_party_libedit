<configuration>
    <oatconfig>
    <licensefile>COPYING</licensefile>
        <filefilterlist>
            <filefilter name="defaultPolicyFilter" desc="Filters for compatibility，license header policies">
                <filteritem type="filename" name="COPYING|compile|configure|ChangeLog|configure.ac|depcomp|install-sh|INSTALL|missing||Makefile.am|THANKS" desc="文件不涉及license"/>
                <filteritem type="filename" name="*.m4|config.*" desc="文件不涉及license"/>
                <filteritem type="filepath" name="doc/mdoc2man.awk" desc="文件不涉及license"/>
                <filteritem type="filepath" name="doc/.*.roff" desc="文件不涉及license"/>
                <filteritem type="filepath" name="src/.*" desc="文件不涉及license"/>
                <filteritem type="filepath" name="examples/fileman.c" desc="文件不涉及license"/>
                <filteritem type="filepath" name="examples/fuzz1.c" desc="文件不涉及license"/>
                <filteritem type="filepath" name="examples/tc1.c" desc="文件不涉及license"/>
                <filteritem type="filepath" name="examples/wtc1.c" desc="文件不涉及license"/>
                <filteritem type="filename" name="libedit.spec" desc="spec文件不涉及license"/>
            </filefilter>
            <filefilter name="copyrightPolicyFilter" desc="Filters for copyright header policies">
                <filteritem type="filename" name="configure|configure.ac|compile|depcomp|install-sh|missing|Makefile.am|THANKS" desc="InvalidCopyright file"/>
                <filteritem type="filename" name="*.m4|config.*" desc="InvalidCopyright file"/>
                <filteritem type="filepath" name="doc/mdoc2man.awk" desc="文件不涉及license"/>
                <filteritem type="filepath" name="doc/.*.roff" desc="InvalidCopyright file"/>
                <filteritem type="filepath" name="src/.*" desc="InvalidCopyright file"/>
                <filteritem type="filepath" name="examples/fileman.c" desc="InvalidCopyright file"/>
                <filteritem type="filepath" name="examples/fuzz1.c" desc="InvalidCopyright file"/>
                <filteritem type="filepath" name="examples/tc1.c" desc="InvalidCopyright file"/>
                <filteritem type="filepath" name="examples/wtc1.c" desc="InvalidCopyright file"/>
            </filefilter>
        </filefilterlist>
    </oatconfig>
</configuration>
